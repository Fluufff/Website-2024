{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "unused-imports"
  ],
  "rules": {
    "import/order": [
      "warn",
      {
        "alphabetize": { "order": "asc" },
        "newlines-between": "always"
      }
    ],
    "unused-imports/no-unused-imports": "warn"
  }
}
